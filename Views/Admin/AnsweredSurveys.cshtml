@model List<QuestionnaireApp.Models.AnsweredSurvey>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@


@foreach (var answeredSurvey in Model)
{
    <div>
        <p>User ID: @answeredSurvey.UserId</p>
        <p>Survey ID: @answeredSurvey.SurveyId</p>
        <p>User Answers:</p>
        <ul>
            @foreach (var answer in answeredSurvey.UserAnswers)
            {
                <li>@answer</li>
            }
        </ul>
    </div>
}
